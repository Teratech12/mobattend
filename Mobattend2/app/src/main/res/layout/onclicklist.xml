<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_event"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.sky_phase.mobattend.EventActivity">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/addperson"
        android:layout_alignStart="@+id/addperson"
        android:layout_marginTop="45dp"
        android:id="@+id/deleteperson" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/deleteperson"
        android:layout_alignStart="@+id/deleteperson"
        android:layout_marginTop="47dp"
        android:id="@+id/editclass" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editclass"
        android:layout_alignStart="@+id/editclass"
        android:layout_marginTop="30dp"
        android:id="@+id/deleteclass" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:id="@+id/addperson"
        android:layout_below="@+id/takeattendance"
        android:layout_alignStart="@+id/takeattendance" />

    <TextView
        android:text="Take "
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="105dp"
        android:layout_marginTop="77dp"
        android:id="@+id/takeattendance"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
